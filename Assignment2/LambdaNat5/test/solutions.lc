let is_empty = \x. if x=# then 1 else 0
in
is_empty a:# // 0

;;

let rec fib = \x. if x=0 then 0 else if x=1 then 1 else (fib (x - 1) + fib (x - 2))
in
fib 6 // 8

;;

let rec length = \x. if x=# then 0 else 1 + (tl x)
in
length a:b:c:# // 3

;;

let rec even = \x. if x=# then 1 else if (tl x)=# then 0 else if (tl (tl x))=# then 1 else (even (tl (tl x)))
in
even a1:a2:a3:a4:# // 1

;;

let rec append = \x.\y. if (hd x)=# then y else (ECons (append (tl x)) (hd x))
in
let rec reverse = \z. if z=# then # else (append (hd z) (reverse (tl z)))
in
reverse a:b:c:# // c:b:a:#

;;

let rec weave = \x. \y. if x=# then if y=# then # else y else if y=# then x else if ((hd x) less_equal (hd y))=1 then (ECons (hd x) (weave (tl x) y)) else (ECons (hd y) (weave x (tl y)))
in
weave 1:2:3:# 4:5:6:#
